{
    "compilerOptions": {
        "target": "es2018",
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": false,
        "esModuleInterop": true,
        "sourceMap": true,
        "declaration": true,
        "lib": [
            "es2017", "ES2019.Object", "ES2019.Array",
        ],
        "noUnusedLocals": true,
        "outDir": "dist",
        "forceConsistentCasingInFileNames": true,
    },
    "include": ["src/"]
}
